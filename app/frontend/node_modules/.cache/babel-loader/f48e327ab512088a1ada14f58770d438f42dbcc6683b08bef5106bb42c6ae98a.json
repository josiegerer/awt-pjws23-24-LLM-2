al @createUser=\"createUser\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport UserCreateModal from './UserCreateModal.vue';\r\n\r\nexport default {\r\n  components: {\r\n    UserCreateModal,\r\n  },\r\n  props: [\"user\"],\r\n  data() {\r\n    return {\r\n      users: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchUsers();\r\n  },\r\n  methods: {\r\n    async fetchUsers() {\r\n      try {\r\n        const response = await axios.get('http://localhost:5000/get_users');\r\n        this.users = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching users:', error);\r\n      }\r\n    },\r\n    createUser(username) {\r\n      axios.post('http://localhost:5000/add_user', {username})\r\n      .then(response => {\r\n        console.log(response.data);\r\n        this.fetchUsers();\r\n      })\r\n      .catch(error => {\r\n        console.error(\"Error creating user\", error);\r\n      })\r\n    },\r\n    navigateToUserChats(userID) {\r\n      console.log('Navigating to UserChats for user ID:', userID);\r\n      this.$router.push({ name: \"user-chats\", params: { id: userID } });\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.user-box {\r\n  display: inline-block;\r\n  margin: 50px;\r\n  padding: 50px;\r\n  border-radius: 50%;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  background-color: #ffffff;\r\n  text-align: center;\r\n  text-decoration: none;\r\n  color: #333;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.user-box:hover {\r\n  background-color: #f0f0f0;\r\n}\r\n\r\ndiv.user-creation{\r\n  margin: 50px;\r\n  text-align: center;\r\n  padding: 20px 20px;\r\n}\r\n\r\n</style>\r\n"],"mappings":";AAqBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,eAAc,MAAO,uBAAuB;AAEnD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAK,EAAE,CAAC,MAAM,CAAC;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACD